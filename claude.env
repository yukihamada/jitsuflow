# JitsuFlow Environment Variables - Claude Development
# ブラジリアン柔術トレーニング＆道場予約アプリ

# Application
APP_NAME=JitsuFlow
APP_VERSION=1.0.0
ENVIRONMENT=development

# API Configuration
API_BASE_URL=http://localhost:8787/api
API_TIMEOUT=30000

# Database (Cloudflare D1)
DATABASE_NAME=jitsuflow_db
DATABASE_ID=placeholder

# Authentication
JWT_SECRET=jitsuflow-dev-jwt-secret-key-2024
JWT_EXPIRATION=24h

# Stripe Payment (Test Keys)
STRIPE_PUBLISHABLE_KEY=pk_test_placeholder
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_placeholder

# Cloudflare
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token

# R2 Storage
R2_BUCKET_NAME=jitsuflow-assets
R2_ACCESS_KEY_ID=placeholder
R2_SECRET_ACCESS_KEY=placeholder

# KV Storage
KV_NAMESPACE_ID=placeholder

# Email Service (Development)
EMAIL_SERVICE=console
EMAIL_API_KEY=placeholder
EMAIL_FROM=dev@jitsuflow.app

# Notification Services (Development)
SLACK_WEBHOOK_URL=your_slack_webhook_url
GOOGLE_CHAT_WEBHOOK_URL=https://chat.googleapis.com/v1/spaces/placeholder/messages

# Analytics
GOOGLE_ANALYTICS_ID=G-PLACEHOLDER
CLOUDFLARE_ANALYTICS_TOKEN=placeholder

# Development
DEV_PORT=3000
DEV_API_PORT=8787
DEBUG=true
LOG_LEVEL=debug

# Security
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8787
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=1000

# Feature Flags
ENABLE_PREMIUM_FEATURES=true
ENABLE_VIDEO_UPLOAD=true
ENABLE_LIVE_STREAMING=false
ENABLE_ANALYTICS=true