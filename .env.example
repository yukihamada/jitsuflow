# JitsuFlow Environment Variables
# Copy this file to .env and fill in your values

# Environment
NODE_ENV=development
ENVIRONMENT=development

# API Configuration
API_BASE_URL=http://localhost:8787/api

# Cloudflare Configuration
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_API_TOKEN=your_api_token_here
CLOUDFLARE_ZONE_ID=your_zone_id_here

# Cloudflare Workers
WORKER_NAME=jitsuflow-worker
WORKER_ROUTE=jitsuflow.app/api/*

# Cloudflare D1 Database
D1_DATABASE_NAME=jitsuflow_db
D1_DATABASE_ID=placeholder_will_be_set_by_script

# Cloudflare R2 Storage
R2_BUCKET_NAME=jitsuflow-assets
R2_ACCESS_KEY_ID=your_r2_access_key_here
R2_SECRET_ACCESS_KEY=your_r2_secret_key_here

# Cloudflare KV Storage
KV_NAMESPACE_NAME=jitsuflow-sessions
KV_NAMESPACE_ID=placeholder_will_be_set_by_script

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_here_min_32_chars
JWT_EXPIRES_IN=7d

# Stripe Configuration
STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PRICE_ID_MONTHLY=price_your_monthly_price_id
STRIPE_PRICE_ID_ANNUAL=price_your_annual_price_id

# AI Configuration
OPENAI_API_KEY=your_openai_api_key_here
GROQ_API_KEY=your_groq_api_key_here
DEEPFACE_API_KEY=your_deepface_api_key_here

# Email Configuration (ReSend)
RESEND_API_KEY=your_resend_api_key_here
RESEND_FROM_EMAIL=noreply@jitsuflow.app

# Google Services
GOOGLE_CHAT_WEBHOOK_URL=your_google_chat_webhook_url
GOOGLE_ANALYTICS_ID=your_ga_id_here

# Flutter Web Configuration
FLUTTER_WEB_PORT=3000

# Security
ALLOWED_ORIGINS=http://localhost:3000,https://jitsuflow.app
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Feature Flags
ENABLE_AI_ANALYSIS=true
ENABLE_FACE_RECOGNITION=true
ENABLE_DEEPFAKE_DETECTION=true
ENABLE_VIDEO_MORPHING=false

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info